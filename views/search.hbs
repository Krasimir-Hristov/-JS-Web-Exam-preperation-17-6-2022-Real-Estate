<form class="search" action="/search" method="get">
    <input type="search" name="search" placeholder="Search here...">
    <button type="submit">Search</button>
</form>
<section id="find-section">


    <div class="card">
        {{#each houses}}
        <h3>{{name}}</h3>
        <p>{{description}} </p>

        <div class="cta-container"><a href="/catalog/{{_id}}" class="det-link">Details</a></div>
        <div class="card_circle"><img src="/static/images/house2.jpg"></div>

    </div>
    {{else}}
    <div class="no-data-listing">
        <p class="no-offer">No match was found for the submitted type{{search}}</p>
    </div>
    {{/each}}
</section>